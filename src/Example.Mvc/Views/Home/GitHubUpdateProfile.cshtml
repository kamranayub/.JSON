@model dynamic[]
@{ ViewBag.Title = "GitHub - Update Profile (POST) Example"; }
<h2>
    GitHub - Update Profile (POST) Example</h2>

@if (Model != null)
{
    <h3>
        Updated Blog Value</h3>

    @Model[0].user.blog

    <h3>
        Reverted Blog Value</h3>

    @Model[1].user.blog
}
else
{
    <p>@ViewBag.Error</p>
}